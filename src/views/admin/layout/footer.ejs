<script>
    const lineChartData = {
        labels: ['Jan', 'Feb', 'Mar', 'Apr'], // Sesuaikan dengan data periode
        datasets: [{
            label: 'Pendapatan',
            data: [1000, 1200, 1500, 1700], // Ganti dengan data dinamis
            backgroundColor: 'rgba(75, 192, 192, 0.2)',
            borderColor: 'rgba(75, 192, 192, 1)',
            borderWidth: 1
        }]
    };

    const barChartData = {
        labels: ['Jan', 'Feb', 'Mar', 'Apr'], // Sesuaikan dengan data periode
        datasets: [{
            label: 'Sampah (Kg)',
            data: [200, 250, 300, 400], // Ganti dengan data dinamis
            backgroundColor: 'rgba(54, 162, 235, 0.2)',
            borderColor: 'rgba(54, 162, 235, 1)',
            borderWidth: 1
        }]
    };

    new Chart(document.getElementById('canvas-linechart'), {
        type: 'line',
        data: lineChartData,
    });

    new Chart(document.getElementById('canvas-barchart'), {
        type: 'bar',
        data: barChartData,
    });
</script>
    
